     "text": [
      "  User  Transportation mode      Distance (km)\n",
      "------  ---------------------  ---------------\n",
      "   175  bus                          1.80771\n",
      "   163  taxi                        11.3794\n",
      "   167  walk                         0.455136\n",
      "   167  bike                         4.0598\n",
      "   128  car                         18.5745\n",
      "   062  run                          0.0332532\n",
      "   128  train                       19.0923\n",
      "   128  subway                      10.8537\n",
      "   128  airplane                  4391.42\n",
      "   128  boat                        65.5548\n"
     ]
